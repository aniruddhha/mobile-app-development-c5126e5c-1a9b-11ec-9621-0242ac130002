<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aniruddha.kudalkar.jetpacknavigationgraph">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JetpackNavigationGraph">
        <activity
            android:name=".hammn.NvDrActivity"
            android:exported="true"
            android:label="@string/title_activity_nv_dr"
            android:theme="@style/Theme.JetpackNavigationGraph.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".adv.BtnNavActivity"
            android:exported="true"
            android:label="@string/title_activity_btn_nav"/>

        <activity
            android:name=".fundamentals.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.JetpackNavigationGraph.NoActionBar" />
    </application>

</manifest>